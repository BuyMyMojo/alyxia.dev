{
  "name": "alyxia.dev",
  "namespace": false,
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators"
  ],
  "uncurried": true,
  "ppx-flags": [],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "warnings": {
    "error": "+8"
  },
  "suffix": ".mjs",
  "bsc-flags": [
    "-bs-g",
    "-open RescriptCore"
  ],
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}